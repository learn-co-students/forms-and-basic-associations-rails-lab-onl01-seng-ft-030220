<h1>Create a Song</h1>
<%= form_for @song do |f| %>
  <%= f.label :title, "Song Title"%>
  <%= f.text_field :title %><br>
  <%= f.label :name, "Artist Name"%>
  <%= f.text_field :artist_name %><br>
  <%= f.label "Genre" %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
  <%= f.label :note_contents %>
  <input type="text" name="song[note_contents][]" id="song_notes_attributes_0_content">
  <input type="text" name="song[note_contents][]" id="song_notes_attributes_1_content">
  <%= f.fields_for :notes do |note| %>
  <%= note.label :note %>
  <%= note.text_area :content , rows: 2, cols: 50 %>
  <% end %>
  <%= f.submit %>
<% end %>